<div class="col-xs-12">
  <table class="table table-striped">
    <thread>
      <tr>
        <th>パート</th>
        <th>名前</th>
        <th>回答</th>
        <th>コメント</th>
      </tr>
    </thread>
    <tbody>
      <% users.each do |user| %>
        <tr>
          <td><%= user.part %></td>
          <td><%= user.name %></td>
            <% if user.reaction?(schedule) %>
              <% his_answer = user.answers.find_by(schedule_id: schedule.id) %>
              <td><%= his_answer.status %></td>
              <td><%= his_answer.comment %></td>
            <% else %>
              <td>ー</td>
              <td></td>
            <% end %>
        </tr>
      <% end %>
    </tbody>
     <%= paginate users %>
  </table>
</div>